@using HelloApp.Models
@model IEnumerable<Phone>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" charset="utf-8" />
    <title>Home Page</title>
</head>
<body>
    <table>
        @foreach (Phone p in Model)
        {
            <tr>
                <td>@p.Name</td>
                <td>@p.Manufacturer?.Name</td>
                <td>@p.Price</td>
            </tr>
        }
    </table>
</body>
</html>
